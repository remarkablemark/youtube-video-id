{
  "name": "youtube-video-id",
  "version": "0.0.4",
  "description": "Extracts the YouTube video ID from a url or string.",
  "author": "Mark <mark@remarkablemark.org>",
  "main": "index.js",
  "scripts": {
    "build": "rollup --config --failAfterWarnings",
    "clean": "rm -rf coverage dist",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:ci": "jest --ci --coverage --colors",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remarkablemark/youtube-video-id"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/youtube-video-id/issues"
  },
  "keywords": [
    "youtube",
    "video",
    "id",
    "url",
    "string"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.4",
    "@rollup/plugin-terser": "0.4.3",
    "eslint": "8.49.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.6.4",
    "rollup": "3.29.0"
  },
  "files": [
    "/dist",
    "index.js"
  ],
  "license": "MIT"
}
